{
    "copyright": "Copyright 2016 Ayogo Health Inc.",
    "compilerOptions": {
        "declaration": false,
        "inlineSourceMap": true,
        "inlineSources": true,
        "lib": ["es5", "dom"],
        "module": "es2015",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noLib": false,
        "outDir": "dist-es5",
        "pretty": true,
        "removeComments": true,
        "strictNullChecks": true,
        "stripInternal": true,
        "target": "es5"
    },

    "include": [
        "./src/**/*.ts"
    ]
}
